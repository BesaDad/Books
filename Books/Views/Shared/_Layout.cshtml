<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Библиотека</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!--This bundle was moved by the Telerik VS Extensions for compatibility reasons-->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Библиотека", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Домашняя страница", "Index", "Home")</li>
                </ul>
            </div>
        </div>
    </div>
    <div id="container" class="container body-content">
        <div id="successAlert" class="alert alert-success" style="display: none;">
            <div class="row">
                <i class="fa fa-check-circle col-md-1" aria-hidden="true"></i>
                <a href="#" class="close" data-dismiss="alert">&times;</a><strong>Успешно! </strong><span></span>
            </div>
        </div>
        <div id="errorAlert" class="alert alert-danger" style="display: none;">
            <div class="row">
                <i class="fa fa-times-circle col-md-1" aria-hidden="true"></i>
                <a href="#" class="close" data-dismiss="alert">&times;</a><strong>Ошибка! </strong><span></span>
            </div>
        </div>
        <div id="errorAlertFixed" class="alert alert-danger" hidden="hidden" style="display: none;">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <ul class="list-unstyled"></ul>
        </div>
        @RenderBody()
        <hr />
        <footer></footer>
    </div>

    <div class="modal fade bs-example-modal-lg" id="dialogModalLg" tabindex="-1" role="dialog" aria-labelledby="dialogModalLgLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
            </div>
        </div>
    </div>
    @Scripts.Render("~/bundles/inputmask")
    @Scripts.Render("~/bundles/bookJs")
    <script type="text/javascript">
    </script>

    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", false)
</body>
</html>
